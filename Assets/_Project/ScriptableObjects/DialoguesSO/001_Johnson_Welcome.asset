  Dialogue:
    Phrases:
    - Character: {fileID: 11400000, guid: 193e45979ffb4ac4daaf67a79de1faa7, type: 2}
      Text: "Ah, Senhor Prefeito, que prazer t\xEA-lo aqui conosco! Sou Johnson,
